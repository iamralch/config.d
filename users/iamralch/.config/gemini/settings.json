{
  "ui": {
    "theme": "./thems/catppuccin-mocha.json",
    "hideBanner": true,
    "hideTips": true,
    "useFullWidth": true,
    "showMemoryUsage": true,
    "showLineNumbers": true,
    "enablePromptCompletion": true
  },
  "general": {
    "vimMode": true,
    "preferredEditor": "neovim"
  },
  "privacy": {
    "usageStatisticsEnabled": false
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "tools": {
    "shell": {
      "showColor": true
    }
  },
  "context": {
    "fileName": [
      "AGENTS.md",
      "CLAUDE.md",
      "GEMINI.md"
    ]
  }
}
